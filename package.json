{
  "name": "@oreillymedia/launchbot-mdl",
  "version": "1.2.4",
  "description": "Styleguide for Launchbot",
  "license": "Apache-2.0",
  "author": "Martin Bravo",
  "repository": "github.com/odewahn/launchbot-mdl",
  "main": "dist/material-design-lite.min.css",
  "engines": {
    "node": ">=4.2.2"
  },
  "scripts": {
    "build": "npm run scss & npm run cleancss",
    "scss": "node-sass src/material-design-lite.scss -o dist/",
    "cleancss": "cleancss -o dist/material-design-lite.min.css dist/material-design-lite.css ",
    "test": "npm build && git status | grep 'working directory clean' >/dev/null || (echo 'Please commit all changes generated by building'; exit 1)"
  },
  "devDependencies": {
    "clean-css": "^3.4.20",
    "node-sass": "^3.10.1"
  }
}
